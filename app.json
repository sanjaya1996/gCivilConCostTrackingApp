{
  "expo": {
    "name": "GcivilCon",
    "description": "Cost tracking app for construction project of GreenCivilCon company",
    "slug": "greencivilcon-costtrackingapp",
    "privacy": "public",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.bnpgroupkoi.cost-tracking-greencivilcon",
      "buildNumber": "1.0.1",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.bnpgroupkoi.cost_tracking_greencivilcon",
      "versionCode": 2,
      "useNextNotificationsApi": true,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    }
  }
}
